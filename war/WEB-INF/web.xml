<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<filter>
   <filter-name>ObjectifyFilter</filter-name>
   <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
 </filter>

<filter-mapping>
   <filter-name>ObjectifyFilter</filter-name>
   <url-pattern>/*</url-pattern>
</filter-mapping>
<servlet>
	<servlet-name>SignupUser</servlet-name>
	<servlet-class>com.btp.SignupUser</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>SignupUser</servlet-name>
	<url-pattern>/SignupUser</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>LoginUser</servlet-name>
	<servlet-class>com.btp.LoginUser</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>LoginUser</servlet-name>
	<url-pattern>/LoginUser</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Logout</servlet-name>
	<servlet-class>com.btp.Logout</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Logout</servlet-name>
	<url-pattern>/Logout</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>TestimonialServlet</servlet-name>
	<servlet-class>com.btp.TestimonialServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>TestimonialServlet</servlet-name>
	<url-pattern>/TestimonialServlet</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>TestimonialEntry</servlet-name>
	<servlet-class>com.btp.TestimonialEntry</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>TestimonialEntry</servlet-name>
	<url-pattern>/TestimonialEntry</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>DiseaseEntryServlet</servlet-name>
	<servlet-class>com.btp.DiseaseEntryServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>DiseaseEntryServlet</servlet-name>
	<url-pattern>/DiseaseEntryServlet</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>SearchServlet</servlet-name>
	<servlet-class>com.btp.SearchServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>SearchServlet</servlet-name>
	<url-pattern>/SearchServlet</url-pattern>
</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
